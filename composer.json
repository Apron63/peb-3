{
    "type": "project",
    "license": "proprietary",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=8.2",
        "ext-ctype": "*",
        "ext-dom": "*",
        "ext-iconv": "*",
        "ext-xmlreader": "*",
        "ext-zip": "*",
        "beberlei/doctrineextensions": "dev-master",
        "doctrine/doctrine-bundle": "^2.11",
        "doctrine/doctrine-migrations-bundle": "^3.2",
        "doctrine/orm": "^2.7",
        "jonasarts/tcpdf-bundle": "*",
        "knplabs/knp-paginator-bundle": "^6.3",
        "phpdocumentor/reflection-docblock": "*",
        "phpoffice/phpspreadsheet": "*",
        "phpoffice/phpword": "*",
        "stof/doctrine-extensions-bundle": "^1.8",
        "symfony/asset": "^7.0",
        "symfony/cache": "^7.0",
        "symfony/clock": "^7.0",
        "symfony/config": "^7.0",
        "symfony/console": "^7.0",
        "symfony/dependency-injection": "^7.0",
        "symfony/doctrine-bridge": "^7.0",
        "symfony/doctrine-messenger": "^7.0",
        "symfony/dom-crawler": "^7.0",
        "symfony/dotenv": "^7.0",
        "symfony/error-handler": "^7.0",
        "symfony/event-dispatcher": "^7.0",
        "symfony/expression-language": "^7.0",
        "symfony/filesystem": "^7.0",
        "symfony/finder": "^7.0",
        "symfony/flex": "^2",
        "symfony/form": "^7.0",
        "symfony/framework-bundle": "^7.0",
        "symfony/http-client": "^7.0",
        "symfony/http-foundation": "^7.0",
        "symfony/intl": "^7.0",
        "symfony/mailer": "^7.0",
        "symfony/messenger": "^7.0",
        "symfony/mime": "^7.0",
        "symfony/monolog-bridge": "^7.0",
        "symfony/monolog-bundle": "^3.0",
        "symfony/notifier": "^7.0",
        "symfony/options-resolver": "^7.0",
        "symfony/password-hasher": "^7.0",
        "symfony/process": "^7.0",
        "symfony/property-access": "^7.0",
        "symfony/property-info": "^7.0",
        "symfony/routing": "^7.0",
        "symfony/runtime": "^7.0",
        "symfony/security-bundle": "^7.0",
        "symfony/security-core": "^7.0",
        "symfony/security-csrf": "^7.0",
        "symfony/security-http": "^7.0",
        "symfony/serializer": "^7.0",
        "symfony/string": "^7.0",
        "symfony/translation": "^7.0",
        "symfony/twig-bridge": "^7.0",
        "symfony/twig-bundle": "^7.0",
        "symfony/validator": "^7.0",
        "symfony/var-dumper": "^7.0",
        "symfony/var-exporter": "^7.0",
        "symfony/web-link": "^7.0",
        "symfony/webpack-encore-bundle": "^2.0",
        "symfony/yaml": "^7.0",
        "trsteel/ckeditor-bundle": "^1.18",
        "twig/extra-bundle": "^3",
        "twig/twig": "3.*",
        "xpanel/mobile-detect-bundle": "*"
    },
    "config": {
        "allow-plugins": {
            "symfony/flex": true,
            "symfony/runtime": true
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php80": "*",
        "symfony/polyfill-php81": "*",
        "symfony/polyfill-php82": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "6.3.*"
        }
    },
    "require-dev": {
        "phpstan/phpdoc-parser": "^1.15",
        "phpunit/phpunit": "^10.3",
        "symfony/browser-kit": "^7.0",
        "symfony/css-selector": "^7.0",
        "symfony/debug-bundle": "^7.0",
        "symfony/maker-bundle": "^1.48",
        "symfony/phpunit-bridge": "^7.0",
        "symfony/stopwatch": "^7.0",
        "symfony/web-profiler-bundle": "^7.0",
        "symfony/http-kernel": "^7.0"
    }
}

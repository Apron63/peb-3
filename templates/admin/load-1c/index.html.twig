{% extends 'admin/base.html.twig' %}

{% block title %}Загрузка пользователей из 1С{% endblock %}

{% block content %}

    <h3>Загрузить пользователей из 1С</h3>

    {{ form_start(form, {'attr':{'class': 'form-contact contact_form'}}) }}

    <div class="row">

        <div class="col-10 mb-3">
            <div class="form-group">
                <span class="text-danger">{{ form_errors(form.filename) }}</span>
                {{ form_widget(form.filename) }}
            </div>
        </div>

        <div class="form-group mb-3">
            {{ form_widget(form.submit) }}
        </div>

    </div>

    {{ form_end(form) }}

{% endblock %}